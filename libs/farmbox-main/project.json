{
  "name": "farmbox-main",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/farmbox-main/src",
  "prefix": "farmbox",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/farmbox-main/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/farmbox-main/**/*.ts",
          "libs/farmbox-main/**/*.html"
        ]
      }
    }
  },
  "tags": []
}
